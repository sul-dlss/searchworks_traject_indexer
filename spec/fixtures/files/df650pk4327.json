ructuredValue":[],"parallelValue":[],"groupedValue":[],"value":"Sanderson","type":"surname","identifier":[],"note":[],"appliesTo":[]}],"parallelValue":[],"groupedValue":[],"identifier":[],"note":[],"appliesTo":[]}],"type":"person","role":[{"structuredValue":[],"parallelValue":[],"groupedValue":[],"value":"author","code":"aut","uri":"http://id.loc.gov/vocabulary/relators/aut","identifier":[],"source":{"code":"marcrelator","uri":"http://id.loc.gov/vocabulary/relators/","note":[]},"note":[],"appliesTo":[]}],"identifier":[],"affiliation":[],"note":[],"parallelContributor":[]},{"name":[{"structuredValue":[{"structuredValue":[],"parallelValue":[],"groupedValue":[],"value":"Tom","type":"forename","identifier":[],"note":[],"appliesTo":[]},{"structuredValue":[],"parallelValue":[],"groupedValue":[],"value":"Cramer","type":"surname","identifier":[],"note":[],"appliesTo":[]}],"parallelValue":[],"groupedValue":[],"identifier":[],"note":[],"appliesTo":[]}],"type":"person","role":[{"structuredValue":[],"parallelValue":[],"groupedValue":[],"value":"author","code":"aut","uri":"http://id.loc.gov/vocabulary/relators/aut","identifier":[],"source":{"code":"marcrelator","uri":"http://id.loc.gov/vocabulary/relators/","note":[]},"note":[],"appliesTo":[]}],"identifier":[],"affiliation":[],"note":[],"parallelContributor":[]}],"event":[{"structuredValue":[],"type":"deposit","date":[{"structuredValue":[],"parallelValue":[],"groupedValue":[],"value":"2015-04-03","type":"publication","encoding":{"code":"edtf","note":[]},"identifier":[],"note":[],"appliesTo":[]}],"contributor":[],"location":[],"identifier":[],"note":[],"parallelEvent":[]},{"structuredValue":[],"type":"creation","date":[{"structuredValue":[],"parallelValue":[],"groupedValue":[],"value":"2015-05","type":"creation","encoding":{"code":"edtf","note":[]},"identifier":[],"note":[],"appliesTo":[]}],"contributor":[],"location":[],"identifier":[],"note":[],"parallelEvent":[]}],"form":[{"structuredValue":[{"structuredValue":[],"parallelValue":[],"groupedValue":[],"value":"Text","type":"type","identifier":[],"note":[],"appliesTo":[]},{"structuredValue":[],"parallelValue":[],"groupedValue":[],"value":"Article","type":"subtype","identifier":[],"note":[],"appliesTo":[]}],"parallelValue":[],"groupedValue":[],"type":"resource type","identifier":[],"source":{"value":"Stanford self-deposit resource types","note":[]},"note":[],"appliesTo":[]},{"structuredValue":[],"parallelValue":[],"groupedValue":[],"value":"text","type":"resource type","identifier":[],"source":{"value":"MODS resource types","note":[]},"note":[],"appliesTo":[]},{"structuredValue":[],"parallelValue":[],"groupedValue":[],"value":"Text","type":"resource type","identifier":[],"source":{"value":"DataCite resource types","note":[]},"note":[],"appliesTo":[]}],"geographic":[],"language":[],"note":[{"structuredValue":[],"parallelValue":[],"groupedValue":[],"value":"The International Image Interoperability Framework (http://iiif.io) is a community of academic and national libraries, research institutions, museums, archives, nonprofits and commercial organizations that are committed to interoperable image delivery on the web. IIIF has defined common application programming interfaces (APIs) for image and metadata delivery, has supported the development of best-in-class image delivery and comparison software, and has cultivated a growing and sustainable community model for interoperable image repositories. This paper provides an overview of IIIF, with a focus on its origins, rationale and benefits to users and institutions. We describe the motivating use cases and applications for image interoperability, walk through the technical framework and supported APIs, and highlight IIIF-compatible software for image analysis, comparison and annotation.  We conclude with a discussion of the IIIF community process and future directions.","type":"abstract","identifier":[],"note":[],"appliesTo":[]},{"structuredValue":[],"parallelValue":[],"groupedValue":[],"value":"